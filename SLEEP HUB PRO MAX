local data = supported[placeId]

if data then
    local success, err = pcall(function()
        loadstring(game:HttpGet(data.url))()
    end)

    if success then
        print("Loaded: " .. data.name)
    else
        warn("Script load failed for " .. data.name .. " ‚Üí " .. tostring(err))
        game.Players.LocalPlayer:Kick("‡∏î‡∏≤‡∏ß‡∏ô‡πå‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à")
