repeat task.wait() until game:IsLoaded()

local placeId = game.PlaceId

local isKayakAndSurf         = placeId == 136404558442020
local isArcadeSlamSimulator  = placeId == 134457821643846
local isChickenCannonSim     = placeId == 103007810968052

if isKayakAndSurf then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sleephub-pro/SLRRP-HUB/refs/heads/main/Kayak%20and%20Surf"))()
    print("Loaded: Kayak and Surf Script")

elseif isArcadeSlamSimulator then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sleephub-pro/SLRRP-HUBV1/refs/heads/main/Arcade%20Slam%20Simulator"))()
    print("Loaded: Arcade Slam Simulator Script")

elseif isChickenCannonSim then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sleephub-pro/SLRRP-HUBV1/refs/heads/main/Chicken%20Cannon%20Simulator"))()
    print("Loaded: Chicken Cannon Simulator Script")

else
    game.Players.LocalPlayer:Kick("ไม่รองรับเกมนี้")
    task.wait(3)
    game:Shutdown()
end
