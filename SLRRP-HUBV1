local ScoreGoal = game.PlaceId == 129944202116127

repeat wait() until game:IsLoaded()


if BrainrotClick then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nook184/SLEEP-HUB/refs/heads/main/%2B1%20Brainrot%20Per%20Click"))()
    print("Loaded: +1 Brainrot Per Click Script")
    
elseif ThrowBasketball then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nook184/SLEEP-HUB/refs/heads/main/Throw%20a%20Basketball"))()
    print("Loaded: Throw a Basketball Script")
    
elseif SkateboardSim then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nook184/SLEEP-HUB/refs/heads/main/Skateboard%20Simulator!"))()
    print("Loaded: Skateboard Simulator Script")

elseif ScoreGoal then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sleephub-pro/SLRRP-HUB/refs/heads/main/Score%20a%20Goal!.txt"))()
    print("Loaded: Score a Goal! Script")
    
else

    game.Players.LocalPlayer:Kick("ไม่รองรับเกมนี้")
    wait(3)
    game:Shutdown()
    wait(3)
    if game then
        while true do end
    end
end
